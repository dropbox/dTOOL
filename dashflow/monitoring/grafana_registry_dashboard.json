{
  "title": "DashFlow Package Registry - Production Monitoring",
  "tags": [
    "dashflow",
    "registry",
    "packages",
    "api",
    "production"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Request Rate (QPS)",
      "type": "stat",
      "gridPos": { "x": 0, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "sum(rate(dashflow_registry_http_requests_total[5m]))",
          "legendFormat": "QPS"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 100, "color": "yellow" },
              { "value": 500, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"] }
      }
    },
    {
      "id": 2,
      "title": "Success Rate",
      "type": "stat",
      "gridPos": { "x": 4, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "sum(rate(dashflow_registry_http_requests_total{status=~\"2..\"}[5m])) / sum(rate(dashflow_registry_http_requests_total[5m])) * 100",
          "legendFormat": "Success"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "red" },
              { "value": 95, "color": "yellow" },
              { "value": 99, "color": "green" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"] }
      }
    },
    {
      "id": 3,
      "title": "P99 Latency",
      "type": "stat",
      "gridPos": { "x": 8, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(dashflow_registry_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 0.5, "color": "yellow" },
              { "value": 1, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"] }
      }
    },
    {
      "id": 4,
      "title": "Cache Hit Rate",
      "type": "stat",
      "gridPos": { "x": 12, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "sum(rate(dashflow_registry_cache_hits_total[5m])) / (sum(rate(dashflow_registry_cache_hits_total[5m])) + sum(rate(dashflow_registry_cache_misses_total[5m]))) * 100",
          "legendFormat": "Hit Rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "red" },
              { "value": 70, "color": "yellow" },
              { "value": 90, "color": "green" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"] }
      }
    },
    {
      "id": 5,
      "title": "In-Flight Requests",
      "type": "stat",
      "gridPos": { "x": 16, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "dashflow_registry_http_requests_in_flight",
          "legendFormat": "In-Flight"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 50, "color": "yellow" },
              { "value": 100, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"] }
      }
    },
    {
      "id": 6,
      "title": "Total Packages",
      "type": "stat",
      "gridPos": { "x": 20, "y": 0, "w": 4, "h": 4 },
      "targets": [
        {
          "expr": "dashflow_registry_packages_total",
          "legendFormat": "Packages"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "blue" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"] }
      }
    },

    {
      "id": 10,
      "title": "HTTP Request Metrics",
      "type": "row",
      "gridPos": { "x": 0, "y": 4, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 11,
      "title": "Request Rate by Endpoint",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 5, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(dashflow_registry_http_requests_total[5m])) by (path)",
          "legendFormat": "{{path}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "spanNulls": false,
            "stacking": { "mode": "none" }
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 12,
      "title": "Latency Heatmap",
      "type": "heatmap",
      "gridPos": { "x": 12, "y": 5, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(increase(dashflow_registry_http_request_duration_seconds_bucket[1m])) by (le)",
          "format": "heatmap",
          "legendFormat": "{{le}}"
        }
      ],
      "options": {
        "calculate": false,
        "yAxis": { "unit": "s" },
        "color": { "scheme": "Spectral", "reverse": true }
      }
    },
    {
      "id": 13,
      "title": "Response Status Distribution",
      "type": "piechart",
      "gridPos": { "x": 0, "y": 13, "w": 6, "h": 8 },
      "targets": [
        {
          "expr": "sum(increase(dashflow_registry_http_requests_total[1h])) by (status)",
          "legendFormat": "{{status}}"
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "values": ["percent", "value"] },
        "pieType": "donut"
      }
    },
    {
      "id": 14,
      "title": "Error Rate by Endpoint",
      "type": "timeseries",
      "gridPos": { "x": 6, "y": 13, "w": 9, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(dashflow_registry_http_requests_total{status=~\"4..|5..\"}[5m])) by (path, status)",
          "legendFormat": "{{path}} ({{status}})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80,
            "stacking": { "mode": "normal" }
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 15,
      "title": "Latency Percentiles",
      "type": "timeseries",
      "gridPos": { "x": 15, "y": 13, "w": 9, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(dashflow_registry_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P50"
        },
        {
          "expr": "histogram_quantile(0.90, sum(rate(dashflow_registry_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P90"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(dashflow_registry_http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "P99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 0
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },

    {
      "id": 20,
      "title": "Cache Performance",
      "type": "row",
      "gridPos": { "x": 0, "y": 21, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 21,
      "title": "Cache Hit/Miss Rate by Type",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 22, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(dashflow_registry_cache_hits_total[5m])) by (cache_type)",
          "legendFormat": "{{cache_type}} hits"
        },
        {
          "expr": "sum(rate(dashflow_registry_cache_misses_total[5m])) by (cache_type)",
          "legendFormat": "{{cache_type}} misses"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "sum"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 22,
      "title": "Cache Hit Rate by Type",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 22, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(dashflow_registry_cache_hits_total[5m])) by (cache_type) / (sum(rate(dashflow_registry_cache_hits_total[5m])) by (cache_type) + sum(rate(dashflow_registry_cache_misses_total[5m])) by (cache_type)) * 100",
          "legendFormat": "{{cache_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 30
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "red" },
              { "value": 70, "color": "yellow" },
              { "value": 90, "color": "green" }
            ]
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 23,
      "title": "Cache Size",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 30, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "dashflow_registry_cache_size_bytes",
          "legendFormat": "{{cache_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 24,
      "title": "Cache Entry Count",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 30, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "dashflow_registry_cache_entries",
          "legendFormat": "{{cache_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },

    {
      "id": 30,
      "title": "Storage Operations (S3/R2)",
      "type": "row",
      "gridPos": { "x": 0, "y": 36, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 31,
      "title": "Storage Operations by Type",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 37, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(dashflow_registry_storage_operations_total[5m])) by (operation, backend)",
          "legendFormat": "{{backend}} {{operation}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 20
          }
        }
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 32,
      "title": "Storage Latency (P99)",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 37, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(dashflow_registry_storage_operation_duration_seconds_bucket[5m])) by (le, operation, backend))",
          "legendFormat": "{{backend}} {{operation}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 0
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 33,
      "title": "Storage Throughput",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 45, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "sum(rate(dashflow_registry_storage_bytes_total[5m])) by (direction, backend)",
          "legendFormat": "{{backend}} {{direction}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 30
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 34,
      "title": "Storage Errors",
      "type": "timeseries",
      "gridPos": { "x": 12, "y": 45, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "sum(rate(dashflow_registry_storage_errors_total[5m])) by (operation, backend)",
          "legendFormat": "{{backend}} {{operation}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "green" },
              { "value": 0.1, "color": "yellow" },
              { "value": 1, "color": "red" }
            ]
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },

    {
      "id": 40,
      "title": "Search & Authentication",
      "type": "row",
      "gridPos": { "x": 0, "y": 51, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 41,
      "title": "Search Queries by Type",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 52, "w": 8, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(dashflow_registry_search_queries_total[5m])) by (search_type)",
          "legendFormat": "{{search_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 30
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 42,
      "title": "Search Latency (P99)",
      "type": "timeseries",
      "gridPos": { "x": 8, "y": 52, "w": 8, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(dashflow_registry_search_query_duration_seconds_bucket[5m])) by (le, search_type))",
          "legendFormat": "{{search_type}} P99"
        },
        {
          "expr": "histogram_quantile(0.50, sum(rate(dashflow_registry_search_query_duration_seconds_bucket[5m])) by (le, search_type))",
          "legendFormat": "{{search_type}} P50"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 0
          }
        }
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 43,
      "title": "API Key Verifications",
      "type": "timeseries",
      "gridPos": { "x": 16, "y": 52, "w": 8, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(dashflow_registry_api_key_verifications_total[5m])) by (result)",
          "legendFormat": "{{result}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 30
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "invalid" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }]
          },
          {
            "matcher": { "id": "byName", "options": "valid" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 44,
      "title": "API Key Cache Efficiency",
      "type": "stat",
      "gridPos": { "x": 0, "y": 60, "w": 6, "h": 4 },
      "targets": [
        {
          "expr": "rate(dashflow_registry_api_key_cache_hits_total[5m]) / (rate(dashflow_registry_api_key_cache_hits_total[5m]) + rate(dashflow_registry_api_key_cache_misses_total[5m])) * 100",
          "legendFormat": "Hit Rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": 0, "color": "red" },
              { "value": 70, "color": "yellow" },
              { "value": 90, "color": "green" }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": { "values": false, "calcs": ["lastNotNull"] }
      }
    },
    {
      "id": 45,
      "title": "Rate Limiting",
      "type": "timeseries",
      "gridPos": { "x": 6, "y": 60, "w": 9, "h": 6 },
      "targets": [
        {
          "expr": "sum(rate(dashflow_registry_rate_limit_events_total[5m])) by (result)",
          "legendFormat": "{{result}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "bars",
            "fillOpacity": 80,
            "stacking": { "mode": "normal" }
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "limited" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }]
          },
          {
            "matcher": { "id": "byName", "options": "allowed" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "id": 46,
      "title": "Search Results Distribution",
      "type": "histogram",
      "gridPos": { "x": 15, "y": 60, "w": 9, "h": 6 },
      "targets": [
        {
          "expr": "sum(increase(dashflow_registry_search_results_count_bucket[1h])) by (le, search_type)",
          "format": "heatmap",
          "legendFormat": "{{search_type}} {{le}}"
        }
      ],
      "options": {
        "legend": { "displayMode": "list", "placement": "right" }
      }
    },

    {
      "id": 50,
      "title": "Packages & Contributions",
      "type": "row",
      "gridPos": { "x": 0, "y": 66, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 51,
      "title": "Package Operations",
      "type": "timeseries",
      "gridPos": { "x": 0, "y": 67, "w": 8, "h": 8 },
      "targets": [
        {
          "expr": "sum(rate(dashflow_registry_package_operations_total[5m])) by (operation)",
          "legendFormat": "{{operation}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 30
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "download" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "blue" } }]
          },
          {
            "matcher": { "id": "byName", "options": "publish" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }]
          },
          {
            "matcher": { "id": "byName", "options": "yank" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" } }]
          }
        ]
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
