{
  "title": "DashFlow Quality Agent - Production Monitoring",
  "tags": [
    "dashflow",
    "quality",
    "llm",
    "production"
  ],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 2,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "Success Rate (Target: ≥90%)",
      "type": "stat",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "(100 * sum(rate(dashstream_quality_monitor_queries_passed_total[5m])) / clamp_min(sum(rate(dashstream_quality_monitor_queries_total[5m])), 0.000000001)) or vector(0)",
          "legendFormat": "Success Rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 85,
                "color": "yellow"
              },
              {
                "value": 90,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 2,
      "title": "Current Quality Score (Target: ≥0.90)",
      "type": "stat",
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "avg(dashstream_quality_monitor_quality_score) or vector(0)",
          "legendFormat": "Avg Quality"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 3,
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 0.85,
                "color": "yellow"
              },
              {
                "value": 0.9,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 4,
      "title": "Average Retries (Target: <1.5)",
      "type": "stat",
      "gridPos": {
        "x": 18,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "(sum(dashstream_quality_retry_count_sum) / clamp_min(sum(dashstream_quality_retry_count_count), 1)) or vector(0)",
          "legendFormat": "Avg Retries"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 2,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 1.5,
                "color": "yellow"
              },
              {
                "value": 2.5,
                "color": "red"
              }
            ]
          }
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "mean"
          ]
        }
      }
    },
    {
      "id": 5,
      "title": "Request Rate (QPS)",
      "type": "graph",
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "sum(rate(dashstream_quality_monitor_queries_total[1m])) or vector(0)",
          "legendFormat": "Total Requests"
        },
        {
          "expr": "sum(rate(dashstream_quality_monitor_queries_passed_total[1m])) or vector(0)",
          "legendFormat": "Successful Requests"
        },
        {
          "expr": "sum(rate(dashstream_quality_monitor_queries_failed_total[1m])) or vector(0)",
          "legendFormat": "Failed Requests"
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "yaxes": [
        {
          "format": "reqps",
          "label": "Requests/sec"
        },
        {
          "format": "short"
        }
      ],
      "legend": {
        "show": true,
        "alignAsTable": true,
        "rightSide": false
      }
    },
    {
      "id": 6,
      "title": "Quality Components (Accuracy, Relevance, Completeness)",
      "type": "graph",
      "gridPos": {
        "x": 12,
        "y": 4,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "dashstream_quality_accuracy or vector(0)",
          "legendFormat": "Accuracy"
        },
        {
          "expr": "dashstream_quality_relevance or vector(0)",
          "legendFormat": "Relevance"
        },
        {
          "expr": "dashstream_quality_completeness or vector(0)",
          "legendFormat": "Completeness"
        },
        {
          "expr": "dashstream_quality_monitor_quality_score or vector(0)",
          "legendFormat": "Overall Quality"
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "yaxes": [
        {
          "format": "short",
          "label": "Quality Score",
          "min": 0,
          "max": 1
        },
        {
          "format": "short"
        }
      ],
      "legend": {
        "show": true,
        "alignAsTable": true,
        "rightSide": false
      }
    },
    {
      "id": 7,
      "title": "Model Usage Distribution",
      "type": "piechart",
      "gridPos": {
        "x": 0,
        "y": 10,
        "w": 8,
        "h": 6
      },
      "targets": [
        {
          "expr": "sum(rate(dashstream_queries_by_model_total{model=\"gpt-4o-mini\"}[5m])) or vector(0)",
          "legendFormat": "Fast Model (gpt-4o-mini)"
        },
        {
          "expr": "sum(rate(dashstream_queries_by_model_total{model=\"gpt-4\"}[5m])) or vector(0)",
          "legendFormat": "Premium Model (gpt-4)"
        }
      ],
      "options": {
        "pieType": "pie",
        "displayLabels": [
          "name",
          "percent"
        ],
        "legend": {
          "show": true,
          "placement": "right"
        }
      }
    },
    {
      "id": 8,
      "title": "Latency Distribution (P50/P95/P99)",
      "type": "graph",
      "gridPos": {
        "x": 8,
        "y": 10,
        "w": 8,
        "h": 6
      },
      "targets": [
        {
          "expr": "(histogram_quantile(0.50, sum(rate(dashstream_query_latency_ms_bucket[5m])) by (le)) and on() (sum(rate(dashstream_query_latency_ms_count[5m])) > 0)) or vector(0)",
          "legendFormat": "P50 Latency"
        },
        {
          "expr": "(histogram_quantile(0.95, sum(rate(dashstream_query_latency_ms_bucket[5m])) by (le)) and on() (sum(rate(dashstream_query_latency_ms_count[5m])) > 0)) or vector(0)",
          "legendFormat": "P95 Latency"
        },
        {
          "expr": "(histogram_quantile(0.99, sum(rate(dashstream_query_latency_ms_bucket[5m])) by (le)) and on() (sum(rate(dashstream_query_latency_ms_count[5m])) > 0)) or vector(0)",
          "legendFormat": "P99 Latency"
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "yaxes": [
        {
          "format": "ms",
          "label": "Latency"
        },
        {
          "format": "short"
        }
      ],
      "legend": {
        "show": true,
        "alignAsTable": true,
        "rightSide": false
      }
    },
    {
      "id": 9,
      "title": "Retry Count Percentiles",
      "type": "graph",
      "gridPos": {
        "x": 16,
        "y": 10,
        "w": 8,
        "h": 6
      },
      "targets": [
        {
          "expr": "(histogram_quantile(0.50, sum(rate(dashstream_quality_retry_count_bucket[5m])) by (le)) and on() (sum(rate(dashstream_quality_retry_count_count[5m])) > 0)) or vector(0)",
          "legendFormat": "P50 Retries"
        },
        {
          "expr": "(histogram_quantile(0.90, sum(rate(dashstream_quality_retry_count_bucket[5m])) by (le)) and on() (sum(rate(dashstream_quality_retry_count_count[5m])) > 0)) or vector(0)",
          "legendFormat": "P90 Retries"
        },
        {
          "expr": "(histogram_quantile(0.99, sum(rate(dashstream_quality_retry_count_bucket[5m])) by (le)) and on() (sum(rate(dashstream_quality_retry_count_count[5m])) > 0)) or vector(0)",
          "legendFormat": "P99 Retries"
        },
        {
          "expr": "(sum(dashstream_quality_retry_count_sum) / clamp_min(sum(dashstream_quality_retry_count_count), 1)) or vector(0)",
          "legendFormat": "Average"
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "yaxes": [
        {
          "format": "short",
          "label": "Count"
        },
        {
          "format": "short"
        }
      ],
      "legend": {
        "show": true,
        "alignAsTable": true,
        "rightSide": false
      },
      "stack": true
    },
    {
      "id": 10,
      "title": "Quality Score Trend (6h)",
      "type": "graph",
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 12,
        "h": 6
      },
      "targets": [
        {
          "expr": "avg(dashstream_quality_monitor_quality_score) or vector(0)",
          "legendFormat": "Average Quality"
        },
        {
          "expr": "0.90",
          "legendFormat": "Target (0.90)"
        }
      ],
      "xaxis": {
        "mode": "time"
      },
      "yaxes": [
        {
          "format": "short",
          "label": "Quality Score",
          "min": 0,
          "max": 1
        },
        {
          "format": "short"
        }
      ],
      "legend": {
        "show": true,
        "alignAsTable": true,
        "rightSide": false
      },
      "alert": {
        "name": "Quality Score Below Target",
        "conditions": [
          {
            "evaluator": {
              "type": "lt",
              "params": [
                0.9
              ]
            },
            "query": {
              "params": [
                "A",
                "5m",
                "now"
              ]
            },
            "reducer": {
              "type": "avg"
            }
          }
        ],
        "frequency": "1m",
        "for": "5m",
        "message": "Quality score has been below 0.90 for 5 minutes",
        "noDataState": "no_data",
        "executionErrorState": "alerting"
      }
    },
