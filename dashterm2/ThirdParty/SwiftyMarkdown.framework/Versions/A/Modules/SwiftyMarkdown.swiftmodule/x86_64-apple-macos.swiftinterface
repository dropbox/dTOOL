  case trailingSide
  public static func == (a: SwiftyMarkdown.SpaceAllowed, b: SwiftyMarkdown.SpaceAllowed) -> Swift.Bool
  public func hash(into hasher: inout Swift.Hasher)
  public var hashValue: Swift.Int {
    get
  }
}
public enum Cancel {
  case none
  case allRemaining
  case currentSet
  public static func == (a: SwiftyMarkdown.Cancel, b: SwiftyMarkdown.Cancel) -> Swift.Bool
  public func hash(into hasher: inout Swift.Hasher)
  public var hashValue: Swift.Int {
    get
  }
}
public enum CharacterRuleTagType {
  case open
  case close
  case metadataOpen
  case metadataClose
  case repeating
  public static func == (a: SwiftyMarkdown.CharacterRuleTagType, b: SwiftyMarkdown.CharacterRuleTagType) -> Swift.Bool
  public func hash(into hasher: inout Swift.Hasher)
  public var hashValue: Swift.Int {
    get
  }
}
public struct CharacterRuleTag {
  public init(tag: Swift.String, type: SwiftyMarkdown.CharacterRuleTagType)
}
public struct CharacterRule : Swift.CustomStringConvertible {
  public let primaryTag: SwiftyMarkdown.CharacterRuleTag
  public let tags: [SwiftyMarkdown.CharacterRuleTag]
  public let escapeCharacters: [Swift.Character]
  public let styles: [Swift.Int : any SwiftyMarkdown.CharacterStyling]
  public let minTags: Swift.Int
  public let maxTags: Swift.Int
  public var metadataLookup: Swift.Bool
  public var isRepeatingTag: Swift.Bool {
    get
  }
  public var definesBoundary: Swift.Bool
  public var shouldCancelRemainingRules: Swift.Bool
  public var balancedTags: Swift.Bool
  public var description: Swift.String {
    get
  }
  public func tag(for type: SwiftyMarkdown.CharacterRuleTagType) -> SwiftyMarkdown.CharacterRuleTag?
  public init(primaryTag: SwiftyMarkdown.CharacterRuleTag, otherTags: [SwiftyMarkdown.CharacterRuleTag], escapeCharacters: [Swift.Character] = ["\\"], styles: [Swift.Int : any SwiftyMarkdown.CharacterStyling] = [:], minTags: Swift.Int = 1, maxTags: Swift.Int = 1, metadataLookup: Swift.Bool = false, definesBoundary: Swift.Bool = false, shouldCancelRemainingRules: Swift.Bool = false, balancedTags: Swift.Bool = false)
}
public protocol CharacterStyling {
  func isEqualTo(_ other: any SwiftyMarkdown.CharacterStyling) -> Swift.Bool
}
public enum TokenType {
  case repeatingTag
  case openTag
  case intermediateTag
  case closeTag
  case string
  case escape
  case replacement
  public static func == (a: SwiftyMarkdown.TokenType, b: SwiftyMarkdown.TokenType) -> Swift.Bool
  public func hash(into hasher: inout Swift.Hasher)
  public var hashValue: Swift.Int {
    get
  }
}
public struct Token {
  public let id: Swift.String
  public let type: SwiftyMarkdown.TokenType
  public let inputString: Swift.String
  public var metadataStrings: [Swift.String]
  public var group: Swift.Int {
    get
  }
  public var characterStyles: [any SwiftyMarkdown.CharacterStyling] {
    get
  }
  public var count: Swift.Int {
    get
  }
  public var shouldSkip: Swift.Bool {
    get
  }
  public var tokenIndex: Swift.Int {
    get
  }
  public var isProcessed: Swift.Bool {
    get
  }
  public var isMetadata: Swift.Bool {
    get
  }
  public var children: [SwiftyMarkdown.Token]
  public var outputString: Swift.String {
    get
  }
  public init(type: SwiftyMarkdown.TokenType, inputString: Swift.String, characterStyles: [any SwiftyMarkdown.CharacterStyling] = [])
}
